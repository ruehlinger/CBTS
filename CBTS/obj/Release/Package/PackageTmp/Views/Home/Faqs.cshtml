    ViewBag.Title = "VBA CBTS";
    var tilestyle = ViewBag.Dev ? "thumbnail" : "thumbnaildisabled thumbnail disabled";

}

@section styles
{
    }
    <div class="indxLft2">
        <div class="overview">
            <h2>FAQs</h2>
            <br />
            <p>
                Answers to commonly asked questions.
            </p>
        </div>
    </div>
   


<div class="pagecontent1">
    <h3>How do I:</h3>
   
    <p><ul>
        <li>Enroll in the Competency-based Training System (CBTS)</li>
        <li>Track My Progress</li>
        <li>Add Courses</li>
        <li>Delete Courses</li>
        <li>Get Help</li>
